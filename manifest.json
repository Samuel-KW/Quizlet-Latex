{
  "name": "Quizlet Latex",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Add Latex support to Quizlet sets",
  "homepage_url": "https://github.com/Samuel-KW/Quizlet-Latex",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "contentSettings"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://quizlet.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["src/inject/index.js", "src/inject/mathjax.js", "src/inject/output/*"],
    "matches": ["<all_urls>"]
  }]
}